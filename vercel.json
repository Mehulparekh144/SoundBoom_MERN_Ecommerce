{
    "buildCommand" : "cd client && npm install && ./node_modules/vite/bin/vite.js build" , 
    "outputDirectory" : "client/dist" ,
    "framework" : "vite",
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
    "rewrites" : [
        {
            "source" : "/api/(.*)",
            "destination" : "/api/index.js"
        }
    ]
}